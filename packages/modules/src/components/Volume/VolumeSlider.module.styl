@import '~@expandorg/uikit/colors'
@import '~@expandorg/uikit/font'

.container
  box-sizing: border-box

  position: absolute
  bottom: 60px

  width: 60px
  height 237px

  display: flex
  align-items: center
  justify-content: center

  padding: 20px 0

  z-index: 2

  border-top-left-radius: 5px
  border-top-right-radius: 5px

  background-color: color-light-blue
  border: 1px solid color-heavy-blue;
